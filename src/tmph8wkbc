use std::thread;
use std::sync::mpsc::channel;
use std::sync::{Arc,Mutex};


fn main() {
     let (tx,rx) = channel();
  let data = Arc::new(Mutex::new(vec![1,2,3]))
     for i in 0..3{
     	let (data,tx) = (data.clone(),tx.clone());
     thread::spawn(move||{
     	let mut data = data.lock().unwrap();
     	*data[0] +=i
     		tx.send(data[0]).unwrap();
     	});
     }

     for _ in 0..10{
     	println!("{}",rx.recv().unwrap());
     }
}
