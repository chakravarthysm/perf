use std::ops::Add;
#[derive(Debug)]
struct Sheep<'c> {
  age: &'c i32,
}

impl Add for Sheep

impl<'c> Sheep<'c> {
    fn grow_old(&mut self)  {
       self.age+=100;
    }                    
}
fn main() {
    let  a = 3;
    let mut s = Sheep { age: &a };
    s.grow_old();
    println!("{:?};", s)
}